{
  "name": "@ckirby/dom-dom-dom-dommmm",
  "version": "6.0.4",
  "description": "DOM/JSDOM wrapper",
  "engines": {
    "node": ">=12.20.0"
  },
  "main": "src/index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "compile": "yarn compile:cjs && yarn compile:esm",
    "compile:cjs": "tsc -p tsconfig.json",
    "compile:esm": "tsc -p tsconfig-esm.json",
    "prepublishOnly": "yarn compile",
    "watch": "yarn tsc-watch",
    "test": "yarn lint && yarn test:tape:difflet",
    "test:ci": "yarn compile:cjs && yarn lint && yarn test:tape",
    "test:tape": "tape `find tests -name '*-test.js'`",
    "test:tape:difflet": "yarn test:tape | tap-difflet --pessimistic",
    "lint": "eslint src tests",
    "release": "tp-release",
    "jsdoc": "jsdoc ./src/*.js -d docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chadkirby/splice-chars.git"
  },
  "author": "Chad Kirby",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.1.0",
    "jsdom": "^18.0.1",
    "@types/node": "^16.3.3",
    "@types/tape": "^4.13.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "tap-difflet": "^0.7.1",
    "tape": "^5.3.1",
    "tape-promise": "^4.0.0",
    "tom-jumbo-grumbo": "git+ssh://git@github.com/PatentNavigation/tom-jumbo-grumbo.git#v1.0.5",
    "ts-node": "^10.1.0",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/chadkirby/splice-chars/issues"
  },
  "homepage": "https://github.com/chadkirby/splice-chars#readme",
  "directories": {
    "test": "tests"
  },
  "files": [
    "setup",
    "dist",
    "types"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
